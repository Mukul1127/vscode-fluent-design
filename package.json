{
  "capabilities": {
    "untrustedWorkspace": {
      "supported": false,
      "description": "Extension modifies core VSCode files."
    }
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-fluent-design.install",
        "title": "Fluent Design: Install"
      },
      {
        "command": "vscode-fluent-design.uninstall",
        "title": "Fluent Design: Uninstall"
      }
    ]
  },
  "description": "Fluent Design language patch for VSCode",
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@types/node": "24.2.0",
    "@types/vscode": "1.93.0",
    "@vscode/vsce": "3.6.0",
    "esbuild": "0.25.8",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "glob": "11.0.3",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.0"
  },
  "displayName": "Fluent Design",
  "engines": {
    "vscode": "^1.93.0"
  },
  "extensionKind": [
    "ui"
  ],
  "keywords": [
    "Fluent",
    "Windows 11",
    "Mica"
  ],
  "license": "GPL-3.0-only",
  "main": "./dist/extension.js",
  "name": "vscode-fluent-design",
  "preview": true,
  "publisher": "Mukul1127",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mukul1127/vscode-fluent-design"
  },
  "scripts": {
    "check": "tsc",
    "clean": "(rm -r dist || true) && (rm *.vsix || true)",
    "compile": "bun esbuild.ts",
    "format": "prettier . --write",
    "lint": "eslint --fix",
    "package": "bun esbuild.ts --production && vsce package --no-dependencies",
    "watch": "bun esbuild.ts --watch"
  },
  "type": "module",
  "version": "1.0.0"
}
